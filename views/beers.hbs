<div id="beers-container">
     {{#each beers}}
      <div id="beer-wrapper">
          <a href="/beers/{{id}}">
             <img src="{{image_url}}" alt="beer image" id="beer-image">
          </a>
          <h3 class="beer-name">{{name}}</h3>
          <p class="tagline">{{tagline}}</p>
          <p class="description-para">{{description}}</p>
      </div>
     {{/each}}

</div>